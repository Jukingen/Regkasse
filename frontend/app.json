{
  "expo": {
    "name": "Cash Register",
    "slug": "cash-register",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "cashregister",
    "userInterfaceStyle": "automatic",
    "description": "RKSV Compliant Cash Register for Cashiers",
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.registrierkasse.cashregister"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.registrierkasse.cashregister"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "fonts": [
      {
        "asset": "assets/fonts/OCRA-B.ttf",
        "family": "OCRA-B"
      }
    ]
  }
}
